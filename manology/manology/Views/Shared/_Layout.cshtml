<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
	@Styles.Render("~/Content/bootstrap/css")
	@Styles.Render("~/Content/css")
</head>
<body>
	<div class="page-wrapper">
		<div class="navbar navbar-fixed-top">
			<div class="navbar-inner navbar-header">
				<div class="container-fluid">
					<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</a>
					<img src="~/Content/images/manology-logo.png" class="pull-left logo-image" />
					<a class="brand brand-custom" href="javascript:;">Manology</a>
					<div class="nav-collapse collapse">
						<p class="navbar-text pull-right"><a href='#' class="navbar-link">Выйти</a></p>
					</div>
				</div>
			</div>
		</div>
		<div class="container-fluid">
			@RenderBody()
		</div>

	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@RenderSection("scripts", required: false)
	</div>
</body>
</html>
